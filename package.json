{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/server.js",
    "server": "nodemon ./dist/server.js",
    "watch": "tsc -w",
    "build": "npm install && tsc && npm run copy-files",
    "dev": "concurrently \"npm run watch\" \"npm run watch-views\" \"npm run server\" ",
    "copy-files": "copyfiles -u 1 \"src/views/**/*\" dist/ ",
    "watch-views": "nodemon --watch src/views -e ejs -x \"npm run copy-files\" "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.56.8",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "prisma": "^6.12.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  }
}
